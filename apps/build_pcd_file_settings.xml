<?xml version="1.0" encoding="utf-8"?>
<DatasetDir>/windows/E/Datasets/rgbd_dataset_freiburg2_pioneer_slam/</DatasetDir>
<SaveDir>~/test/pointclouds/</SaveDir>
<Fovis>
    <Logger>
        <Enabled>true</Enabled>
        <Output>cout</Output>
        <DebugEnabled>true</DebugEnabled>
        <ErrorEnabled>true</ErrorEnabled>
        <MessageEnabled>true</MessageEnabled>
    </Logger>
    <IsEveryCycleKeyframe>true</IsEveryCycleKeyframe>
    <UseMovementThresholdsForKeyframes>true</UseMovementThresholdsForKeyframes>
    <MovementThr>0.1</MovementThr>
    <YawThr>10</YawThr>
    <PublishKeyframeDescriptors>true</PublishKeyframeDescriptors>
</Fovis>
<VoxelGrid>
    <Resolution>0.05</Resolution>
    <Enabled>true</Enabled>
</VoxelGrid>
<PassThrough>
    <LowLimit>0</LowLimit>
    <HighLimit>4</HighLimit>
    <Enabled>true</Enabled>
    <FieldName>z</FieldName>
</PassThrough>
<MaxCycles>1000</MaxCycles>
<Octomap>
    <Logger>
        <Enabled>true</Enabled>
        <Output>cout</Output>
        <DebugEnabled>true</DebugEnabled>
        <ErrorEnabled>true</ErrorEnabled>
        <MessageEnabled>true</MessageEnabled>
    </Logger>
    <VoxelGrid>
        <Enabled>true</Enabled>
        <Resolution>0.05</Resolution>
    </VoxelGrid>
    <HeightFilter>
        <Enabled>false</Enabled>
        <Axis>y</Axis>
        <MaxValue>0</MaxValue>
        <MinValue>0</MinValue>
    </HeightFilter>
    <GroundFilter>
        <Enabled>true</Enabled>
        <Axis>y</Axis>
        <Distance>0.04</Distance>
        <PlaneDistance>0.50</PlaneDistance>
        <Angle>0.15</Angle>
        <AddGroundPointsAsFreeSpace>true</AddGroundPointsAsFreeSpace>
    </GroundFilter>
    <LocalTransform>
        <Enabled>false</Enabled>
        <RobotHeight>0</RobotHeight>
        <RobotHeightAxis>z</RobotHeightAxis>
        <FirstRotationAxis>x</FirstRotationAxis>
        <FirstRotationDegree>90</FirstRotationDegree>
        <SecondRotationAxis>none</SecondRotationAxis>
        <SecondRotationDegree>0</SecondRotationDegree>
    </LocalTransform>
    <PassThroughFilter>
        <Enabled>true</Enabled>
        <LimitMin>0.5</LimitMin>
        <LimitMax>4</LimitMax>
    </PassThroughFilter>
    <UseColor>false</UseColor>
    <MapResolution>0.05</MapResolution>
    <MaxRange>4</MaxRange>
    <LazyEval>false</LazyEval>
</Octomap>
<RobotInfo>
    <AddHeightToOdometry>true</AddHeightToOdometry>
    <ConstantHeight>0</ConstantHeight>
    <Radius>0</Radius>
</RobotInfo>
<iSAM2>
    <Logger>
        <Enabled>true</Enabled>
        <Output>cout</Output>
        <DebugEnabled>true</DebugEnabled>
        <ErrorEnabled>true</ErrorEnabled>
        <MessageEnabled>true</MessageEnabled>
    </Logger>
    <PriorNoise>
        <X>0.1</X>
        <Y>0.1</Y>
        <Z>0.1</Z>
        <Roll>0.1</Roll>
        <Pitch>0.1</Pitch>
        <Yaw>0.1</Yaw>
    </PriorNoise>
    <OdometryNoise>
        <X>0.1</X>
        <Y>0.1</Y>
        <Z>0.1</Z>
        <Roll>0.1</Roll>
        <Pitch>0.1</Pitch>
        <Yaw>0.1</Yaw>
    </OdometryNoise>
    <LoopClosureNoise>
        <X>0.1</X>
        <Y>0.1</Y>
        <Z>0.1</Z>
        <Roll>0.1</Roll>
        <Pitch>0.1</Pitch>
        <Yaw>0.1</Yaw>
    </LoopClosureNoise>
    <UseFeatures>true</UseFeatures>
</iSAM2>
<LoopDetector>
    <NumCandidateImages>10</NumCandidateImages>
    <UseFovisDescriptor>true</UseFovisDescriptor>
    <NormalizeFovisDescriptor>false</NormalizeFovisDescriptor>
    <FeatureDetector>SIFT</FeatureDetector>
    <DescriptorExtractor>SIFT</DescriptorExtractor>
    <MinimumFeatureMatches>50</MinimumFeatureMatches>
    <RANSAC>
        <MinSetSize>3</MinSetSize>
        <MaxSteps>100</MaxSteps>
        <GoodModelPercentage>0.699999988079071</GoodModelPercentage>
        <MinInliers>10</MinInliers>
        <MaxAcceptableError>0.1</MaxAcceptableError>
        <InlierMaxSquaredDist>0.05</InlierMaxSquaredDist>
    </RANSAC>
</LoopDetector>
